{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "debug": false
      }
    ]
  ],
  "plugins": [

    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    "@babel/plugin-proposal-object-rest-spread",
    ["transform-imports", {
      "core-decorators": {
        "transform": "core-decorators/src/${member}"
      }
    }]
  ]
}